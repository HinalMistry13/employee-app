<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.1.xsd">
	
	<grid model="com.axelor.emp.db.Contact" title="Contact" name="contact-grid">
		<field name="type" title="Contact Type"/>
		<field name="contactNumber" title="Contact Number"/>
		<field name="employee" title="Employee" />
	</grid>
	
	<form model="com.axelor.emp.db.Contact" title="Contact" name="contact-form">
		<panel>
			<field name="type"/>
			<field name="contactNumber" min="7" />
			<field name="employee" />
		</panel>
	</form>
	
</object-views>